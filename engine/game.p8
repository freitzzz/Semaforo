pico-8 cartridge // http://www.pico-8.com
version 14
__lua__

--treats a jump call
function jump()
	count_draw=0
	if(is_about_to_jump==1)toast.y-=1,5
	if (is_about_to_jump==2)toast.y+=1
	jump_time+=1
	if(jump_time==10)then
		jump_time=0
		if(is_about_to_jump==2)then
			is_about_to_jump=0
			count_draw=0
			jump_time=0
		end
		if(is_about_to_jump==1)is_about_to_jump=2
	end
end

-- shoots butter
function shoot()
	local E={
		sprite=BUTTER_SPRITE,
		x=toast.x+6,
		y=toast.y+3,
		dx=5,
		dy=0
	}
	add(butter,E)
end

--prepares jump call
function prepare_jump()
	if(is_about_to_jump>=1)then
		count_draw+=1
		if(count_draw==1)jump()
	end
end


--Armando
function mescerda()--mover para a esquerda
	direcao=0
	if(btn(buttons.left)) then
		direcao=1
		if(toast.x<=40)	z-=mv
		if(toast.x>=40) toast.x-=pv
		pos-=1
	end
end
function mdireita()--mover para a direita
	g=mget(pos/8,toast.y/8)
	direcao=0
	if(fget(g,1)==false) then
	if(btn(buttons.right)) then
		direcao=2
		if(toast.x<=88)	toast.x+=pv
		if(toast.x>=88) z+=mv
		pos+=1
		end
		end
end

--Armando

function sprite_1(n_sprites,sprite,x,y,size,contador,velucidade)
	contador[2]+=1
	if (contador[2]==velucidade[contador[1]]) then
		contador[2]=0
		contador[1]+=1
		if (contador[1]==n_sprites) contador[1]=1
	end
	spr(sprite[contador[1]],toast.x,y,size,size)
	return contador
end





--initiates constants and variables
function _init()
	--Armando
	pv=1--velocidade do player
	xmeio=64--meio do ecra em x
	ymeio=64--meio do ecra em x
	mv=0.50-- velocidade do map
	pos=xmeio
	--x=xmeio
	--y=ymeio
	z=0
	loop={1,0} --serve de contador
	loop2={1,0}
	s_player={5,7,9,5,11,3}
	s_player_parado={37,39,41,39}
	s_parado_speed={40,5,10,10}
	s_speed={5,3,2,5,3,2} --deste modo consigo editar otempo que fica em cada sprite
	n_sprites=6
	n_parado_sprites=4+1
	--Freitas
	direcao=0
	toast={x=xmeio,y=ymeio}
	butter={}
	buttons={left=0,right=1,up=2,down=3,shoot_button=4,space=5}
	is_about_to_jump=0
	count_draw=0
	jump_time=0
	BUTTER_SPRITE=0
	TOAST_SPRITE=1
end

--updates actions
function _update()
	--Armando
	mescerda()
	mdireita()
	if (btn()==false) direcao=10
	--Freitas
	for E in all(butter) do
		E.x+=E.dx
		E.y+=E.dy
	end
	if(btn(buttons.up))toast.y+=-1
	if(btn(buttons.down))toast.y+=1
	if(btn(buttons.left))toast.x+=-1
	if(btn(buttons.right))toast.x+=1
	if(btn(buttons.space))is_about_to_jump=1
	if(btn(buttons.shoot_button))shoot()
end

--draws pixels
function _draw()
	cls()
	--spr(TOAST_SPRITE,toast.x,toast.y,2,2)
	prepare_jump()
	for E in all(butter) do
		spr(E.sprite,E.x,E.y)
	end
	--Armando
	map(0+z,0)
	if (direcao==2) loop=sprite_1(n_sprites,s_player,toast.x,toast.y,2,loop,s_speed)
	if (direcao==0) loop2=sprite_1(n_parado_sprites,s_player_parado,toast.x,toast.y,2,loop2,s_parado_speed)
	music(1,6,4)
end

